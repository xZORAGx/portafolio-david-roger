{
  "css.validate": false,
  "postcss.validate": false,
  "tailwindCSS.includeLanguages": {
    "astro": "html"
  },
  "tailwindCSS.experimental.classRegex": [
    ["class:list=\\{([^}]*)\\}", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["classList=\\{([^}]*)\\}", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["class=([\"'`][^\"'`]*[\"'`])", "[\"'`]([^\"'`]*).*?[\"'`]"]
  ],
  "files.associations": {
    "*.astro": "astro"
  },
  "emmet.includeLanguages": {
    "astro": "html"
  }
}
